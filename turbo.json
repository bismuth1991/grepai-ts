{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "outputs": ["src/grepai-config-schema.json", "dist"],
      "cache": true
    },
    "publish:npm": {
      "outputs": [],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "typecheck": {
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  },
  "globalEnv": [],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "NPM_CONFIG_TOKEN",
    "npm_lifecycle_event"
  ]
}
