{
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "tooling/*"
    ],
    "catalog": {
      "@types/bun": "1.3.7",
      "@dotenvx/dotenvx": "1.52.0",
      "typescript": "5.9.3"
    },
    "catalogs": {
      "ai": {
        "@ai-sdk/google": "3.0.7",
        "ai": "6.0.33"
      },
      "effect": {
        "@effect/cli": "0.73.1",
        "@effect/language-service": "0.72.0",
        "@effect/platform": "0.94.2",
        "@effect/platform-bun": "0.87.1",
        "@effect/sql": "0.49.0",
        "@effect/sql-libsql": "0.39.0",
        "@effect/vitest": "0.27.0",
        "effect": "3.19.15"
      }
    }
  },
  "type": "module",
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules",
    "clean:workspaces": "turbo run clean && bun clean",
    "git": "bun run --env-file=.env.development ./tooling/scripts/src git",
    "lint": "turbo lint --continue",
    "lint:fix": "turbo run lint --continue -- --fix",
    "postinstall": "bun --bun tooling/scripts/src/patch-tree-sitter.ts",
    "test": "bun --bun run vitest",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@effect/language-service": "catalog:effect",
    "@effect/vitest": "catalog:effect",
    "@types/bun": "catalog:",
    "oxfmt": "0.27.0",
    "oxlint": "1.42.0",
    "turbo": "2.7.6",
    "vitest": "^3.2.0"
  },
  "peerDependencies": {
    "typescript": "catalog:"
  },
  "packageManager": "bun@1.3.8"
}
