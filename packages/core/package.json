{
  "name": "@grepai/core",
  "exports": {
    ".": "./src/index.ts",
    "./domain": "./src/domain/index.ts",
    "./grepai-config-schema.json": "./src/grep-ai-config-schema.json"
  },
  "scripts": {
    "build": "bun --bun run src/internal/scripts/build.ts && oxfmt",
    "clean": "git clean -xdf .cache .turbo node_modules",
    "format": "oxfmt",
    "lint": "oxlint",
    "test": "bun --bun run vitest -u run",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@ai-sdk/google": "catalog:ai",
    "@ai-sdk/openai": "catalog:ai",
    "@effect/platform": "catalog:effect",
    "@effect/platform-bun": "catalog:effect",
    "@effect/sql": "catalog:effect",
    "@effect/sql-libsql": "catalog:effect",
    "@effect/sql-pg": "catalog:effect",
    "@lancedb/lancedb": "catalog:lancedb",
    "ai": "catalog:ai",
    "apache-arrow": "catalog:lancedb",
    "effect": "catalog:effect",
    "tree-sitter-typescript": "catalog:tree-sitter",
    "web-tree-sitter": "catalog:tree-sitter"
  },
  "devDependencies": {
    "@grepai/tsconfig": "workspace:*",
    "tree-sitter-cli": "catalog:tree-sitter",
    "typescript": "catalog:"
  }
}
