{
  "$schema": "../packages/core/src/grepai-config-schema.json",
  "storage": {
    "type": "lancedb"
  },
  "embedding": {
    "provider": "google",
    "model": "gemini-embedding-001",
    "apiKey": "${GEMINI_API_KEY}",
    "dimensions": 3072,
    "maxChunkSize": 1024,
    "targetChunkSize": 512,
    "tokenizer": "simple"
  },
  "experimental__agentFs": {
    "syncMode": "pull",
    "url": "${AGENT_FS_EFFECT_URL}",
    "authToken": "${AGENT_FS_EFFECT_AUTH_TOKEN}"
  }
}
